#include <stdio.h>
#include <string.h>




int main(){

char input[512];

while(1){
printf("$> ");
fgets(input, 512 ,stdin);
input[strcspn(input, "\n")] = 0;

 if (strcmp(input, "exit") == 0) {
            break;
        }
        
      if (feof(stdin)) {
            break;
        }
        
        char *token = strtok(input, " ");
        while (token != NULL) {
            printf("Token: %s\n", token);
            token = strtok(NULL, " ");
        }

}

return 0;
}
